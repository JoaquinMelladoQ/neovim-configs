"snippets.extends": {
  "typescript": ["javascript"],
  "typescriptreact": ["javascript"]
},

{
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    }
  },
  "codeLens.enable": true
},

{
  "languageserver": {
    "kotlin": {
      "command": "[git clone https://github.com/udalov/kotlin-vim.git]/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  }
},

"languageserver": {
  "golang": {
    "command": "gopls",
    "filetypes": ["go"],
    "initializationOptions": {
      "gocodeCompletionEnabled": true,
      "diagnosticsEnabled": true,
      "lintTool": "golint"
    }
  }
}



{
  "languageserver": {
    "graphql": {
      "disable": true
    }
  }
}

